</IfModule>
# php -- END cPanel-generated handler, do not edit
RewriteCond %{HTTP_HOST} ^rajbariit\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.rajbariit\.com$
RewriteRule ^/?$ "https\:\/\/www\.rajbariit\.com\/index\.html" [R=301,L]

